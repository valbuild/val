{
  "compilerOptions": {
    "esModuleInterop": true,
    "strict": true,
    "allowJs": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "lib": ["es6", "dom"],
    "module": "CommonJS",
    "noEmit": true,
    "target": "ES5",
    "outDir": "dist",
    "rootDir": "src"
  },
  "include": ["src/**/*"],
  "exclude": ["dist/**"]
}
