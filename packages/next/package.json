{
  "name": "@valbuild/next",
  "description": "Val NextJS: hard-coded content - super-charged",
  "private": false,
  "sideEffects": true,
  "keywords": [
    "CMS",
    "next",
    "react"
  ],
  "version": "0.16.0",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "jest"
  },
  "main": "dist/valbuild-next.cjs.js",
  "module": "dist/valbuild-next.esm.js",
  "exports": {
    ".": {
      "module": "./dist/valbuild-next.esm.js",
      "default": "./dist/valbuild-next.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "types": "dist/valbuild-next.cjs.d.ts",
  "preconstruct": {
    "entrypoints": [
      "./index.ts"
    ],
    "exports": true
  },
  "dependencies": {
    "@valbuild/core": "~0.16.0",
    "@valbuild/react": "~0.16.0",
    "@valbuild/server": "~0.16.0"
  },
  "peerDependencies": {
    "next": ">=12.3.4",
    "react": ">=18.2.0"
  },
  "externals": [
    "next"
  ]
}
