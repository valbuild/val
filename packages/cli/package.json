{
  "name": "@valbuild/cli",
  "private": false,
  "version": "0.91.4",
  "description": "Val CLI tools",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valbuild/val.git"
  },
  "bin": {
    "val": "./bin.js"
  },
  "exports": {
