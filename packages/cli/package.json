    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "start": "tsx src/cli.ts --",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@valbuild/core": "~0.91.4",
    "@valbuild/eslint-plugin": "~0.91.4",
    "@valbuild/server": "~0.91.4",
    "@valbuild/shared": "~0.91.4",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "fast-glob": "^3.3.1",
    "meow": "^9.0.0",
    "open": "^9.1.0",
    "picocolors": "^1.0.0",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "prettier": "*",
    "typescript": ">=5.0.0"
  },
