{
  "compilerOptions": {
    "esModuleInterop": true,
    "strict": true,
    "allowJs": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "lib": ["es2020", "dom"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "target": "ES5",
    "outDir": "dist",
    "rootDir": "src",
    "skipLibCheck": true
  },
  "include": ["src/**/*", "../core/src/ValStore.ts"],
  "exclude": ["dist/**", "jsx-runtime/dist/**", "jsx-dev-runtime/dist/**"]
}
