{
  "compilerOptions": {
    "esModuleInterop": true,
    "strict": true,
    "allowJs": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@valbuild/react",
    "lib": [
      "es6",
      "dom"
    ],
    "module": "CommonJS",
    "noEmit": true,
    "target": "ES5",
    "outDir": "dist",
    "rootDir": "src",
    "skipLibCheck": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "dist/**",
    "jsx-runtime/dist/**",
    "jsx-dev-runtime/dist/**"
  ]
}