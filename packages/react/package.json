{
  "name": "@valcms/react",
  "version": "0.1.0",
  "sideEffects": false,
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "echo \"no tests found :(\""
  },
  "dependencies": {
    "@valcms/lib": "^0.1.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0",
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "@babel/core": {
      "optional": true
    },
    "@types/react": {
      "optional": true
    }
  },
  "preconstruct": {
    "entrypoints": [
      "./index.ts",
      "./jsx-runtime.js",
      "./jsx-dev-runtime.js"
    ],
    "umdName": "valReact",
    "exports": {
      "envConditions": [
        "browser",
        "worker"
      ]
    }
  },
  "main": "dist/valcms-react.cjs.js",
  "module": "dist/valcms-react.esm.js",
  "browser": {
    "./dist/valcms-react.esm.js": "./dist/valcms-react.browser.esm.js"
  },
  "exports": {
    ".": {
      "module": {
        "worker": "./dist/valcms-react.worker.esm.js",
        "browser": "./dist/valcms-react.browser.esm.js",
        "default": "./dist/valcms-react.esm.js"
      },
      "default": "./dist/valcms-react.cjs.js"
    },
    "./jsx-runtime": {
      "module": {
        "worker": "./jsx-runtime/dist/valcms-react-jsx-runtime.worker.esm.js",
        "browser": "./jsx-runtime/dist/valcms-react-jsx-runtime.browser.esm.js",
        "default": "./jsx-runtime/dist/valcms-react-jsx-runtime.esm.js"
      },
      "default": "./jsx-runtime/dist/valcms-react-jsx-runtime.cjs.js"
    },
    "./jsx-dev-runtime": {
      "module": {
        "worker": "./jsx-dev-runtime/dist/valcms-react-jsx-dev-runtime.worker.esm.js",
        "browser": "./jsx-dev-runtime/dist/valcms-react-jsx-dev-runtime.browser.esm.js",
        "default": "./jsx-dev-runtime/dist/valcms-react-jsx-dev-runtime.esm.js"
      },
      "default": "./jsx-dev-runtime/dist/valcms-react-jsx-dev-runtime.cjs.js"
    },
    "./package.json": "./package.json"
  }
}
