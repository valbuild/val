{
  "name": "@val/react",
  "version": "1.0.0",
  "sideEffects": false,
  "dependencies": {
    "@val/lib": "1.0.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0",
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "@babel/core": {
      "optional": true
    },
    "@types/react": {
      "optional": true
    }
  },
  "preconstruct": {
    "entrypoints": [
      "./index.ts",
      "./jsx-runtime.js",
      "./jsx-dev-runtime.js"
    ],
    "umdName": "valReact",
    "exports": {
      "envConditions": [
        "browser",
        "worker"
      ]
    }
  },
  "main": "dist/val-react.cjs.js",
  "module": "dist/val-react.esm.js",
  "browser": {
    "./dist/val-react.esm.js": "./dist/val-react.browser.esm.js"
  },
  "exports": {
    ".": {
      "module": {
        "worker": "./dist/val-react.worker.esm.js",
        "browser": "./dist/val-react.browser.esm.js",
        "default": "./dist/val-react.esm.js"
      },
      "default": "./dist/val-react.cjs.js"
    },
    "./jsx-runtime": {
      "module": {
        "worker": "./jsx-runtime/dist/val-react-jsx-runtime.worker.esm.js",
        "browser": "./jsx-runtime/dist/val-react-jsx-runtime.browser.esm.js",
        "default": "./jsx-runtime/dist/val-react-jsx-runtime.esm.js"
      },
      "default": "./jsx-runtime/dist/val-react-jsx-runtime.cjs.js"
    },
    "./jsx-dev-runtime": {
      "module": {
        "worker": "./jsx-dev-runtime/dist/val-react-jsx-dev-runtime.worker.esm.js",
        "browser": "./jsx-dev-runtime/dist/val-react-jsx-dev-runtime.browser.esm.js",
        "default": "./jsx-dev-runtime/dist/val-react-jsx-dev-runtime.esm.js"
      },
      "default": "./jsx-dev-runtime/dist/val-react-jsx-dev-runtime.cjs.js"
    },
    "./package.json": "./package.json"
  }
}
